{
  "name": "cognitive-insight-engine",
  "version": "3.0.0",
  "description": "Advanced Psychological Pattern Analysis System with 500+ cognitive markers, temporal analysis, and conflict detection",
  "main": "server.js",
  "type": "commonjs",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "node test.js",
    "test:full": "npm test && echo 'âœ… All tests completed successfully'",
    "lint": "eslint public/**.js",
    "format": "prettier --write 'public/**/*.js' '*.js'",
    "build": "echo 'Build complete - Frontend application'",
    "clean": "rm -rf node_modules package-lock.json",
    "reset": "npm run clean && npm install",
    "benchmark": "node benchmark.js",
    "coverage": "jest --coverage",
    "docker:build": "docker build -t cognitive-insight-engine .",
    "docker:run": "docker run -p 3000:3000 cognitive-insight-engine",
    "docker:dev": "docker-compose up",
    "render:deploy": "echo 'Pushing to GitHub will auto-deploy to Render.com'"
  },
  "keywords": [
    "psychology",
    "cognitive",
    "nlp",
    "analysis",
    "insight",
    "cbt",
    "therapy",
    "linguistics",
    "pattern-detection",
    "mental-health",
    "self-reflection"
  ],
  "author": {
    "name": "Cognitive Insight Team",
    "email": "contact@cognitive-insight.dev",
    "url": "https://cognitive-insight.dev"
  },
  "contributors": [
    "AI Assistant <ai@cognitive-insight.dev>"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/cognitive-insight-engine.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/cognitive-insight-engine/issues"
  },
  "homepage": "https://cognitive-insight-engine.vercel.app",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "body-parser": "^1.20.2",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^6.10.0",
    "chalk": "^4.1.2",
    "dotenv": "^16.0.3",
    "chart.js": "^4.4.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "eslint": "^8.44.0",
    "prettier": "^3.0.0",
    "jest": "^29.5.0",
    "supertest": "^6.3.3",
    "axios": "^1.4.0",
    "@types/node": "^20.4.5",
    "@types/express": "^4.17.17",
    "benchmark": "^2.1.4"
  },
  "optionalDependencies": {
    "sqlite3": "^5.1.6",
    "redis": "^4.6.7"
  },
  "files": [
    "public/",
    "server.js",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead"
  ],
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/yourusername"
  },
  "config": {
    "port": 3000,
    "maxTextLength": 10000,
    "rateLimit": {
      "windowMs": 900000,
      "max": 100
    }
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "testMatch": [
      "**/test/**/*.js"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "es2021": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "off"
    }
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 2
  }
}
